{
  "ci": {
    "collect": {
      "numberOfRuns": 1,
      "staticDistDir": "build",
      "url": ["/index.html"]
    },
    "assert": {
      "assertions": {
        "categories:accessibility": ["warn", { "minScore": 0.95 }],
        "categories:performance": ["warn", { "minScore": 0.85 }],
        "categories:best-practices": "off",
        "categories:seo": "off",
        "categories:pwa": "off"
      }
    }
  }
}
